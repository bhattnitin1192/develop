<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-init-method="init" default-destroy-method="destroy"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-autowire="byType"
	 default-autowire-candidates="*Writer"> 

<bean id="logger" class="com.nitin.spring.bean.Logger"  >
</bean>
<bean id="consoleWriter" class="com.nitin.spring.bean.ConsoleWriter">

</bean>

<bean id="fileWriter" class="com.nitin.spring.bean.FileWriter">
</bean>

<bean id="fileWriter2" class="com.nitin.spring.bean.FileWriter">
</bean>

</beans>